{
  "name": "node",
  "version": "1.0.0",
  "description": "Welcome to the repository for the backend of the note-go app. This project is designed to provide a restful API for managing flashcards with a serene and zen-like theme.",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm install && npm run migrate:latest",
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "migrate:make": "knex --knexfile ./knexfile.ts migrate:make",
    "migrate:latest": "knex --knexfile ./knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile ./knexfile.ts migrate:rollback",
    "seed-data": "knex seed:run --knexfile ./knexfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "knex": "^3.1.0",
    "pg": "^8.12.0",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/knex": "^0.16.1",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.6",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
